extends ./layout.jade
include ./mixins/collapsible

block content
	+navigation('Education', 'Education')
	div.container.content
		div.section
			h5 George Mason University
		div.divider
		div.row
			div.section.col.s12.m6
				p.bold Degree
				p BS Computer Science
				p Program GPA: 3.36
				p Overall GPA: 3.27
			div.section.col.s12.m6
				p.bold Awards
				p Dean's List (Spring 2010, Spring 2013, Fall 2013)
				p VGAP Grant (2012, 2013)
		div.section
			h5 Coursework & Projects
		div.section
			ul.collapsible.popout(data-collapsible='expandable')
				+collapsible-row('CS 321 - Software Requirements/Design Modeling')
					P Math Tutor with Teacher and Student Accounts (Java)
				+collapsible-row('CS 332 - Object-Oriented Design and Implementation')
				+collapsible-row('CS 465 - Computer Systems Architecture')
				+collapsible-row('CS 471 - Operating Systems')
				+collapsible-row('CS 475 - Concurrent and Distributed Systems')
					p Concurrent Anagram Solver (Java)
				+collapsible-row('CS 480 - Introduction to Artificial Intelligence')
					p Function Solving Genetic Algorithm (Common LISP)
					p Function Solving Neural Network (Common LISP)
					p 8-Puzzle Solver (Common LISP)
					p Konane Adverserial Search Game Playing A.I (Common LISP)
				+collapsible-row('CS 483 - Analysis of Algorithms')
				+collapsible-row('CS 484 - Data Mining')
					p Analysis of Nationwide University Professor Salaries (WEKA)
				+collapsible-row('CS 499 - Mobile Device Programming')
